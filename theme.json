{
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
