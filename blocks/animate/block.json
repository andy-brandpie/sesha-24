{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"title": "Animation Container",
	"description": "Block to wrap around other blocks to give them a chosen animation",
	"name": "acf/animate",
	"category": "sesha",
	"editorStyle": "file:./block-animate.css",
	"icon": {
			"foreground": "#000",
			"src": "admin-customizer"
	},
	"keywords": [
			"sesha",
			"animate",
			"animation",
			"container"
	],
	"acf": {
			"mode": "edit",
			"renderTemplate": "block-animate.php"
	},
	"supports": {
			"align": false,
			"anchor": false,
			"ariaLabel": false,
			"alignContent": false,
			"alignText": false,
			"alignWide": false,
			"customClassName": false,
			"multiple": true,
			"jsx" : true
	}
}
