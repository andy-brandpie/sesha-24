				},
				{
					"name": "Large",
					"slug": "large",
					"size": "clamp(1.25rem, 1.0376rem + 0.9061vw, 2.125rem)"
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "clamp(1.75rem, 1.4163rem + 1.4239vw, 3.125rem)"
				},
				{
					"name": "Extra Extra Large",
					"slug": "xx-large",
