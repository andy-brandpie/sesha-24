{
		"$schema": "https://schemas.wp.org/trunk/block.json",
		"apiVersion": 2,
		"title": "Video Banner",
		"description": "Video Banner at the top of the page",
		"name": "acf/video-banner",
		"category": "sesha",
		"viewScript": [ "file:./block-video-banner.js", "block-video-banner-script" ],
		"icon": {
			"foreground": "#000",
			"src": "video-alt2"
		},
		"keywords": [
			"sesha",
			"video",
			"banner"
		],
		"acf": {
			"mode": "edit",
			"renderTemplate": "block-video-banner.php"
		},
		"supports": {
			"align": false,
			"anchor": false,
			"ariaLabel": false,
			"alignContent": false,
			"alignText": false,
			"alignWide": false,
			"customClassName": false,
			"color": {
					"text": false,
					"background": false,
					"link": false,
					"gradients": false
			},
			"html": false,
			"multiple": false
		}
}
