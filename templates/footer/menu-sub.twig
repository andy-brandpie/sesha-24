{% if footernav %}
	<ul class="flex flex-col gap-sm list-none">
	{% for item in items %}
		<li class="{{ item.classes | join(' ') }} list-none">
			<a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
			{% if item.children %}
				{% include "footer/menu-sub.twig" with {'items': item.children} %}
			{% endif %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
